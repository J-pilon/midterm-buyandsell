<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- Stylesheets -->
  <link rel="stylesheet" href="/styles/navbar.css" type="text/css" />
  <link rel="stylesheet" href="/styles/users.css" type="text/css" />

  <!-- Jqery -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

  <!-- carousel css -->
  <!-- <link rel="stylesheet" href="/styles/carousel.css" /> -->
  <!-- carousel script -->
  <script src="/scripts/carousel.js"></script>


  <!-- Google Fonts -->
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=Titillium+Web:wght@200;300&display=swap" rel="stylesheet">
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=Bangers&display=swap" rel="stylesheet">

  <!-- Font Awesome -->
  <script src="https://kit.fontawesome.com/03a60bf669.js" crossorigin="anonymous"></script>

  <title>Users Page</title>
</head>
<body>
  <%-include('./partials/_headers') %>

<div id="wrapper">
  <div id="carousel">
    <div id="content">

      <div class="songContainer">


    </div>
    <% for (let song of songs){  %>
      <div id="art_and_info">
        <div>
          <img
          class="item"
          src="https://images.unsplash.com/photo-1514320291840-2e0a9bf2a9ae?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=750&q=80"
          />
        </div>
        <div id="song_info">
          <form action="/username/delete" method="POST">
          <p class="song_part"><%= song.song_name %></p>
          <p class="song_part"><%= song.artist_name %></p>
          <p class="song_part"><%= song.genre %></p>
            <input type="hidden" id="favorite" name="song_name" value=<%= song.song_name %> />
            <button type="submit" class="submit-button">delete</button>
          </form>
        </div>
      </div>
        <% } %>
      </div>
  </div>
  <button id="prev">
    <svg
      xmlns="http://www.w3.org/2000/svg"
      width="24"
      height="24"
      viewBox="0 0 24 24"
    >
      <path fill="none" d="M0 0h24v24H0V0z" />
      <path d="M15.61 7.41L14.2 6l-6 6 6 6 1.41-1.41L11.03 12l4.58-4.59z" />
    </svg>
  </button>
  <button id="next">
    <svg
      xmlns="http://www.w3.org/2000/svg"
      width="24"
      height="24"
      viewBox="0 0 24 24"
    >
      <path fill="none" d="M0 0h24v24H0V0z" />
      <path d="M10.02 6L8.61 7.41 13.19 12l-4.58 4.59L10.02 18l6-6-6-6z" />
    </svg>
  </button>
</div>

</body>
</html>
